import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	    int b=sc.nextInt();
	    int numofstudents=(a/b);
	    int numofleftout=(a%b);
	    System.out.println(numofstudents);
	    System.out.println(numofleftout);
	}
}
